start
 = meta_name+

meta_name
 = "#N" ws* data:line_data {
  return {
    "type": "name",
    "value": data
  }
} 


line_data
 = value:(data:!lb.  { return data })* {
  return value.join("");
} 


// line break
lb 
 = "\n"

// whitespace
ws 
 = [ \t]

// any number of whitespace characters
_
 = ws*
